    {
      "id": "run_steady_state",
      "name": "Steady State Running",
      "category": "running",
      "description": "Continuous running at a comfortable, sustainable pace",
      "difficulty": "beginner",
      "equipment": ["running_shoes"],
      "primaryMetrics": ["duration", "distance", "pace"],
      "optionalMetrics": ["averageHR", "maxHR", "rpe", "elevation"],
      "instructions": [
